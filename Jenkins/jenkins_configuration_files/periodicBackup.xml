<?xml version='1.1' encoding='UTF-8'?>
<org.jenkinsci.plugins.periodicbackup.PeriodicBackupLink plugin="periodicbackup@1.8">
  <fileManagerPlugin class="org.jenkinsci.plugins.periodicbackup.FullBackup">
    <restorePolicy class="org.jenkinsci.plugins.periodicbackup.ReplaceRestorePolicy"/>
    <baseDir>/var/lib/jenkins</baseDir>
    <followSymbolicLinks>false</followSymbolicLinks>
  </fileManagerPlugin>
  <locationPlugins>
    <org.jenkinsci.plugins.periodicbackup.LocalDirectory>
      <enabled>true</enabled>
      <path>/opt/backup-jenkins-data</path>
    </org.jenkinsci.plugins.periodicbackup.LocalDirectory>
  </locationPlugins>
  <storagePlugins>
    <org.jenkinsci.plugins.periodicbackup.TarGzStorage/>
  </storagePlugins>
  <backupNow>false</backupNow>
  <tempDirectory>/tmp</tempDirectory>
  <cron>0 1 *  *  *</cron>
  <cycleQuantity>20</cycleQuantity>
  <cycleDays>20</cycleDays>
</org.jenkinsci.plugins.periodicbackup.PeriodicBackupLink>